{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "src/controllers",
            "src/routers",
            "src/services/",
            "src/utils"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
